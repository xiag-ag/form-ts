{
  "name": "@form-ts/react",
  "description": "",
  "version": "0.2.0",
  "license": "MIT",
  "files": [
    "lib/*",
    "es6/*"
  ],
  "main": "lib/index.js",
  "module": "es6/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "clean": "rimraf lib/ es6/",
    "watch": "tsc --project ./tsconfig.json --watch --pretty",
    "build": "run-s --silent build:es6 build:cjs",
    "build:es6": "tsc --project ./tsconfig.json --noEmitOnError --pretty",
    "build:cjs": "tsc --project ./tsconfig.cjs.json --noEmitOnError --pretty",
    "check": "tsc --project ./tsconfig.json --noEmit --pretty",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@form-ts/core": "0.2.0",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.33",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.1.6"
  }
}
